{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug NestJS",
        "args": ["${workspaceFolder}/src/main.ts"],
        "runtimeArgs": ["-r", "ts-node/register"],
        "cwd": "${workspaceFolder}",
        "protocol": "inspector",
        "sourceMaps": true,
        "console": "integratedTerminal",
        "env": {
          "NODE_ENV": "development"
        }
      }
    ]
  }
  